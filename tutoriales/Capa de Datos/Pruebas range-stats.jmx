          <elementProp name="consulta_url" elementType="Argument">
            <stringProp name="Argument.name">consulta_url</stringProp>
            <stringProp name="Argument.value">/api/v1/analytics/range-stats?from=1625115600000&amp;to=1627793999999</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
            <stringProp name="Argument.desc">Rango junio-julio 2021. Opcional: &amp;measurement=temperatura o &amp;station_id=1</stringProp>
          </elementProp>
        </collectionProp>
        <stringProp name="TestPlan.comments">Cambiar ip_postgres e ip_timescale por las IPs de sus EC2 en AWS</stringProp>
      </Arguments>
